{
  "name": "test-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p ${PORT:=3000}",
    "lint": "DEBUG=eslint:cli-engine eslint \"**/*.{ts,tsx,d.ts}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx,d.ts}\" --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "pretest": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix",
    "postinstall": "test -n \"$NOYARNPOSTINSTALL\" || husky install",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\" && npx husky add .husky/pre-push \"yarn test\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "20.5.0",
    "@types/react-dom": "18.2.7",
    "@types/styled-components": "^5.1.28",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.4",
    "hero-slider": "^3.2.0",
    "next": "13.4.13",
    "next-fonts": "^1.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.0.7",
    "styled-reset": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@next/eslint-plugin-next": "^13.5.4",
    "@types/react": "^18.2.24",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "playwright": "^1.38.1",
    "prettier": "3.0.3",
    "ts-prune": "^0.10.3",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typeconv": "^2.3.1",
    "typescript": "^5.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"pre commit \"",
      "pre-push": "echo \"pre push \""
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,json,tsx,ts}": [
      "prettier --write "
    ]
  }
}
